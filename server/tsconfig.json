{
  "compilerOptions": {
    "target": "ESNext",
    "module": "CommonJS",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "../.output/server",
   "types": ["@nuxt/types", "node"],

    "paths": {
      "#app": ["./.nuxt/types/app"],
      "#imports": ["./.nuxt/types/imports"],
      "#nitro": ["./.nuxt/types/nitro"],
      "#plugins": ["./.nuxt/types/plugins"]
    }
  },
  "include": ["api/**/*.ts"],
  "exclude": ["node_modules"]
}
